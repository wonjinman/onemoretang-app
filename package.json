{
  "name": "nextruck-trucker-webapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf android/build && rm -rf android/app/build",
    "re-install": "rm -rf node_modules && yarn install",
    "android": "ENVFILE=.env.beta react-native run-android --variant=BetaDebug --appIdSuffix=beta.debug",
    "android:local": "ENVFILE=.env.local react-native run-android --variant=BetaDebug --appIdSuffix=beta.debug",
    "ios": "react-native run-ios",
    "ios:local": "react-native run-ios --scheme \"Local\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src/ && prettier --write ./src",
    "build:android:beta:release": "cd android && ENVFILE=.env.beta ./gradlew bundleBetaRelease",
    "build:android:prod:release": "cd android && ENVFILE=.env.prod ./gradlew bundleProdRelease",
    "build:ios:prod:debug": "ENVFILE=.env.prod react-native run-ios",
    "build:ios:beta:debug": "ENVFILE=.env.beta react-native run-ios",
    "build:ios:prod:release": "ENVFILE=.env.prod react-native run-ios --configuration Release",
    "build:ios:beta:release": "ENVFILE=.env.beta react-native run-ios --configuration Staging"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/image-editor": "^2.3.0",
    "@react-native-firebase/app": "^17.3.2",
    "@react-native-firebase/remote-config": "^17.3.2",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/native-stack": "^6.9.9",
    "@sentry/react-native": "^4.15.0",
    "@turf/turf": "^6.5.0",
    "axios": "^1.3.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-native": "0.71.2",
    "react-native-config": "^1.5.0",
    "react-native-device-info": "^10.4.0",
    "react-native-image-picker": "^5.1.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-safearea-height": "^1.0.5",
    "react-native-screens": "^3.20.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-vision-camera": "^2.15.4",
    "react-native-webview": "^11.26.1",
    "react-query": "^3.39.3",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
