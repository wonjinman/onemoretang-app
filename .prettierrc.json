{
  "singleQuote": true,
  "parser": "typescript",
  "trailingComma": "all",
  "semi": false,
  "useTabs": true,
  "tabWidth": 2,
  "printWidth": 100,
  "endOfLine": "lf",
  "jsxBracketSameLine": true,
  "jsxSingleQuote": false,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "arrowParens": "avoid",
  "bracketSpacing": true
}
